{
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "concurrently": "^8.2.2",
    "eleventy-plugin-icons": "^4.4.1",
    "simple-icons": "^11.9.0",
    "slugify": "^1.6.6",
    "tailwindcss": "^3.4.1"
  },
  "scripts": {
    "tailwind:watch": "tailwind -i src/assets/tailwind.css -c tailwind.config.js -o _site/style.css --watch",
    "tailwind:build": "tailwind -i src/assets/tailwind.css -c tailwind.config.js -o _site/style.css",
    "dev": "concurrently \"yarn run tailwind:watch\" \"eleventy --serve\"",
    "build": "yarn run tailwind:build && npx eleventy"
  }
}
